@model project1.ViewModel.MenuIndexViewModel

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Department</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model.Staffs)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.firstName</td>
				<td>@item.lastName</td>
				<td>@item.department</td>
			</tr>
		}
	</tbody>
</table>